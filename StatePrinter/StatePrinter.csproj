<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <RootNamespace>StatePrinting</RootNamespace>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>StatePrinterStrongNameKeyFile_DoNotDelete.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
    <ApplicationIcon>gfx\stateprinter.ico</ApplicationIcon>
    <Product>StatePrinter</Product>
    <Company>Kasper B. Graversen, Inc.</Company>
    <Authors>Kasper B. Graversen</Authors>
    <Copyright>Kasper B. Graversen</Copyright>
    <Description>An open source utility to turn object graphs into strings. Automatic unit testing and ToString() methods. Apache v2.0 License</Description>
    <Version>3.0.0</Version>
    <FileVersion>3.0.312.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

</Project>
